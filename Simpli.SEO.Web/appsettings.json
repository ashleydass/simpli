{
	"Logging": {
		"LogLevel": {
			"Default": "Warning"
		}
	},
	"AllowedHosts": "*",
	"SearchSources": [
		{
			"Source": "Google",
			"SearchUrl": "https://www.google.com/search?q={0}",
			"ResultXPathExpression": "//div[@class='g']//a",
			"ResultHeaderXPathExpression": "//h3[@class]" 
		},
		{
			"Source": "Bing",
			"SearchUrl": "https://www.bing.com/search?q={0}",
			"ResultXPathExpression": "//ol[@id='b_results']/li[@class='b_algo']/h2/a",
			"ResultHeaderXPathExpression": "."
		}
	]
}
